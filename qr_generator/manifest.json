{
  "manifest_version": 2,

  "name": "QRcode Generator",
  "description": "Generator an QR image of your current Url",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "click to generate QR code"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}